{
    "name": "pio_dbc_to_cpp",
    "version": "0.0.2",
    "repository": {
      "type": "git",
      "url": "https://github.com/RCMast3r/pio_dbc_to_cpp.git"
    },
    "export": {
      "include": [
        "*.c",
        "*.cpp",
        "*.h",
        "generator"
      ],
      "exclude": [
        "generator/**/__pycache__"
      ]
    },
    "build": {
      "extraScript": "generator/pio_lib_gen.py",
      "includeDir": "",
      "srcDir": "",
      "srcFilter": [
        "+<*.cpp>",
        "+<*.c>",
        "+<*.h>",
        "+<*.hpp>"
      ]
    },
    "frameworks": "*",
    "platforms": "*"
  }